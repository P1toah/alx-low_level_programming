#include <stdio.h>
#include "dog.h"

/**
 *init_dog - intialize the dogs variable
 *@d: the dogs structure
 *@name: name of the dog
 *@age: dogs age
 *@owner: owner name
 *
 */
void init_dog(struct dog *d, char *name, float age, char *owner)
{
struct dog *d = &my_dog;
my_dog.name = "Poppy";
my_dog.age = 3.5;
my_dog.owner = "Bob";
return (0);
}
